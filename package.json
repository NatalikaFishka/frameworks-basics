{
  "name": "webdriver_project",
  "version": "1.0.0",
  "description": "WebDriver Project",
  "main": "index.js",
  "scripts": {
    "postinstall": "webdriver-manager update",
    "start.hub": "java -jar configs/webdrivers/selenium-server-standalone-3.141.59.jar -role hub",
    "start.node1": "java -Dwebdriver.chrome.driver=./configs/webdrivers/chromedriver.exe -jar configs/webdrivers/selenium-server-standalone-3.141.59.jar -role node -hub http://localhost:4444/grid/register -nodeConfig configs/nodes_configs/node_1.json",
    "start.node2": "java -Dwebdriver.edge.driver=./configs/webdrivers/msedgedriver.exe -jar configs/webdrivers/selenium-server-standalone-3.141.59.jar -role node -hub http://localhost:4444/grid/register -nodeConfig configs/nodes_configs/node_2.json",
    "test": "protractor configs/protractor.config.js"
  },
  "keywords": [
    "webdriver",
    "test",
    "selenium"
  ],
  "author": "Nataliya Okuneva",
  "license": "ISC",
  "devDependencies": {
    "jasmine-spec-reporter": "^5.0.2",
    "protractor": "^5.4.4"
  },
  "dependencies": {
    "server": "^1.0.27"
  }
}